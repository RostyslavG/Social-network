<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Tryzoob</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Tryzoob.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav>
            <div class="logo">
                <img src="images/logo.png" alt="">
                <span class="logo-text">Tryzoob</span>
            </div>
                <img class="colos" src="images/l2.png" alt="" />
            <ul>
                <li></li>
                <a asp-area="" asp-page="/Index">Головна</a>
                <li></li>
                <a asp-area="" asp-page="/Login">Залогінитись</a>
                <li></li>
                <a asp-area="" asp-page="/Login">Контакти</a>
                <li></li>
            </ul>
        </nav>
    </header>

    <main role="main"></main>

    <footer></footer>

    <div class="overlay"></div>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>